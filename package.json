{
  "name": "concolor",
  "version": "0.1.9",
  "author": "Vladyslav Dukhin <vladyslav.dukhin@gmail.com>",
  "description": "Console colors for strings templates in node.js",
  "license": "MIT",
  "keywords": [
    "concolor",
    "metarhia",
    "color",
    "colors",
    "colour",
    "console",
    "terminal",
    "ansi",
    "esc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/metarhia/concolor"
  },
  "bugs": {
    "url": "https://github.com/metarhia/concolor/issues",
    "email": "vladyslav.dukhin@gmail.com"
  },
  "main": "./concolor.js",
  "scripts": {
    "test": "npm run lint && node ./test.js",
    "lint": "eslint . && prettier --debug-check \"**/*.js\" \"**/*.json\" \"**/*.md\" .eslintrc .prettierrc",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" .eslintrc .prettierrc --ignore-path .gitignore"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "readmeFilename": "README.md",
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^3.5.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "1.15.3"
  }
}
