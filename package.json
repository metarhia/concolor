{
  "name": "concolor",
  "version": "0.1.13",
  "author": "Vladyslav Dukhin <vladyslav.dukhin@gmail.com>",
  "description": "Console colors for strings templates in node.js",
  "license": "MIT",
  "keywords": [
    "concolor",
    "metarhia",
    "color",
    "colors",
    "colour",
    "console",
    "terminal",
    "ansi",
    "esc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/metarhia/concolor"
  },
  "bugs": {
    "url": "https://github.com/metarhia/concolor/issues",
    "email": "vladyslav.dukhin@gmail.com"
  },
  "main": "./concolor.js",
  "scripts": {
    "test": "npm run lint && node ./test.js",
    "lint": "eslint . && prettier -c \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\" \"**/*.yml\"",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\" \"**/*.yml\""
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "readmeFilename": "README.md",
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "1.16.x"
  }
}
